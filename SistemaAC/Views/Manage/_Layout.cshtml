@using AuStr = SistemaAC.UserStrings.AuthStrings

@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h2>@AuStr.MaLayTitle</h2>

<div>
    <h4>@AuStr.MaLaySubtitle</h4>
    <hr />
    <div class="row">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}

